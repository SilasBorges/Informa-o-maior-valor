#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
int main(){
    int num1, num2;
    setlocale(LC_ALL,"Portuguese");
    system("color F0");

    printf("Informe dois valores :");
    scanf("%d %d",&num1,&num2);

    if(num1>num2){
        printf("O maior numero é %d",num1);
    }else printf("O maior numero é %d",num2);

    return 0;

}
